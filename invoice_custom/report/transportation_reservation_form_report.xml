        <record id="action_transportation_reservation_form_report" model="ir.actions.report">
            <field name="name">Transportation Form</field>
            <field name="model">transportation.reservation.form</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">invoice_custom.transportation_reservation_view</field>
            <field name="report_file">invoice_custom.transportation_reservation_view</field>
            <field name="binding_type">report</field>
            <field name="print_report_name">('hhhhh.pdf')</field>
            <field name="binding_model_id" ref="model_transportation_reservation_form"/>
            <field name="binding_type">report</field>

        </record>

        <template id="transportation_reservation_view">
            <t t-call="web.html_container">
                <t t-call="web.external_layout" style="background:unset">
                    <div class="page">
                        <style>
                            .page .header-table td{
                            border:none !important;
                            }
                            .page > table tr td:last-child{
                            background:unset !important;
                            }
                            .o_boxed_footer{
                            margin-top:50px !important;

                            }
                        </style>
                        <t t-if="selected_show_reservation">
                            <h4 style="text-align:center">TRANSPORTATION RESERVATION</h4>
                        </t>
                        <t t-if="selected_show_voucher">
                            <h4 style="text-align:center">TRANSPORTATION VOUCHER</h4>
                        </t>

                        <div style="width:100%; border: 1px solid black; padding:1%;">
                            <table style="width:100%; border:none;" class="header-table" cellspacing="0"
                                   cellpadding="0">

                                <tr>
                                    <td style="border-right: none;">
                                        <b>To:</b>
                                    </td>
                                    <td>
                                        <span style="margin-left:50px;" t-esc="transportations_id.name.name"/>
                                    </td>
                                    <td style="border-right: none;">
                                        <b>File No:</b>
                                    </td>
                                    <td>
                                        <span style="margin-left:50px;" t-esc="sale_order_id.name"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <b>Group Name:</b>
                                    </td>
                                    <td>
                                        <span style="margin-left:50px;" t-esc="sale_order_id.customer"/>
                                    </td>
                                    <td style="border-right: none;">
                                        <b>Issue Date:</b>

                                    </td>
                                    <td colspan="3">
                                        <span style="margin-left:50px;"
                                              t-esc="context_timestamp(datetime.datetime.now()).strftime('%d/%m/%Y')"/>
                                    </td>
                                </tr>
                            </table>
                        </div>
                        <br/>
                        <t t-if="transportation_ids">
                            <h6 style="text-align:left">With the following transportation services</h6>
                            <table style="width:100%">
                                <thead>
                                    <tr style="background-color: silver; border: 1px solid;">
                                        <td style="border: 1px black solid; text-align:center;">
                                            #
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            From Date
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            To Date
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Type Of Vehicle
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Pax
                                        </td>
                                        <t t-if="show_note">
                                            <td style="border: 1px black solid; text-align:center;">
                                                Notes
                                            </td>
                                        </t>
                                        <t t-if="show_add_note">
                                            <td style="border: 1px black solid; text-align:center;">
                                                Add. Notes
                                            </td>
                                        </t>
                                        <td style="display:none;">
                                    </td>
                                    </tr>
                                </thead>
                                <t t-set="seq" t-value="1"/>

                                <tr t-foreach="selected_transportation" t-as="t">
                                    <td style="border: 1px solid; text-align:center; padding:5px;">
                                        <span t-esc="seq"/>
                                    </td>

                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="t.get('date_from')">
                                            <span t-esc="t['date_from']"
                                                  t-options='{"widget": "date","format": "dd/MM/yyyy"}'/>
                                        </t>
                                    </td>
                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="t.get('date_from')">
                                            <span t-esc="t['date_to']"/>
                                        </t>
                                    </td>

                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="t.get('vehicles_id')">
                                            <span t-esc="t['vehicles_id'][1]"/>
                                        </t>
                                    </td>
                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="t.get('pax')">
                                            <span t-esc="t['pax']"/>
                                        </t>
                                        <t t-if="not t.get('pax')">
                                            <span>0</span>
                                        </t>
                                    </td>
                                    <t t-if="show_note">
                                        <td style="border: 1px solid; padding:5px;">
                                            <span t-esc="transportations_id.note"/>
                                        </td>
                                    </t>
                                    <t t-if="show_add_note">
                                        <td style="border: 1px solid; padding:5px;">
                                            <span t-esc="transportations_id.additional_note"/>
                                        </td>
                                    </t>
                                    <td style="display:none;">
                                    </td>
                                    <t t-set="seq" t-value="seq +1"/>

                                </tr>
                            </table>
                        </t>

                        <br/>

                        <t t-if="sale_order_id.arrival_departure_expense_ids">
                            <table style="width:100%">
                                <thead>
                                    <tr style="background-color: silver; border: 1px solid;">
                                        <td style="border: 1px black solid; text-align:center;">
                                            #
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Arr / Dep
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Date
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Border
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Flight
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Time
                                        </td>

                                        <td style="border: 1px black solid; text-align:center;">
                                            Pax
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Notes
                                        </td>
                                        <td style="display:none;">

                                        </td>
                                    </tr>
                                </thead>
                                <t t-set="seq" t-value="1"/>

                                <tr t-foreach="sale_order_id.arrival_departure_expense_ids" t-as="arr_dep">
                                    <td style="border: 1px solid; text-align:center; padding:5px;">
                                        <span t-esc="seq"/>
                                    </td>

                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="arr_dep.name">
                                            <span t-esc="arr_dep.name"/>
                                        </t>
                                    </td>
                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="arr_dep.date">
                                            <span t-esc="arr_dep.date"
                                                  t-options='{"widget": "date","format": "dd/MM/yyyy"}'/>
                                        </t>
                                    </td>

                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="arr_dep.border.name">
                                            <span t-esc="arr_dep.border.name"/>
                                        </t>
                                    </td>
                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="arr_dep.flight">
                                            <span t-esc="arr_dep.flight"/>
                                        </t>
                                    </td>
                                    <td style="border: 1px solid; padding:5px; text-align:center;">
                                        <t t-if="arr_dep.time">
                                            <span t-esc="arr_dep.time"
                                                  t-options='{"widget": "float_time"}'/>
                                        </t>
                                        <t t-if="not arr_dep.time">
                                            <span>00:00</span>
                                        </t>
                                    </td>
                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="arr_dep.pax">
                                            <span t-esc="arr_dep.pax"/>
                                        </t>
                                        <t t-if="not arr_dep.pax">
                                            <span>0</span>
                                        </t>
                                    </td>
                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="arr_dep.notes">
                                            <span t-esc="arr_dep.notes"/>
                                        </t>
                                    </td>

                                    <td style="display:none;">
                                    </td>
                                    <t t-set="seq" t-value="seq +1"/>
                                </tr>
                            </table>
                        </t>

                        <br/>

                        <t t-if="sale_order_id.itineraries_expense_ids">
                            <table style="width:100%">
                                <thead>
                                    <tr style="background-color: silver; border: 1px solid;">
                                        <td style="border: 1px black solid; text-align:center;">
                                            #
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Date
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Program
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Type Of Service
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Guide Name
                                        </td>
                                        <td style="display:none;">

                                        </td>
                                    </tr>
                                </thead>
                                <t t-set="seq" t-value="1"/>

                                <tr t-foreach="sale_order_id.itineraries_expense_ids" t-as="program">
                                    <td style="border: 1px solid; text-align:center; padding:5px;">
                                        <span t-esc="seq"/>
                                    </td>

                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="program.from_date">
                                            <span t-esc="program.from_date"
                                                  t-options='{"widget": "date","format": "dd/MM/yyyy"}'/>
                                        </t>
                                    </td>
                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="program.name">
                                            <span t-esc="program.name"/>
                                        </t>
                                    </td>

                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="program.type_service_id.name">
                                            <span t-esc="program.type_service_id.name"/>
                                        </t>
                                    </td>
                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="program.guide_name_id.name">
                                            <span t-esc="program.guide_name_id.name"/>
                                        </t>
                                    </td>
                                    <td style="display:none;">
                                    </td>
                                    <t t-set="seq" t-value="seq +1"/>
                                </tr>
                            </table>
                        </t>
                        <br/>
                        <t t-if="sale_order_id.hotels_expense_ids">
                            <table style="width:100%">
                                <thead>
                                    <tr style="background-color: silver; border: 1px solid;">
                                        <td style="border: 1px black solid; text-align:center;">
                                            #
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Hotel
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Check IN
                                        </td>
                                        <td style="border: 1px black solid; text-align:center;">
                                            Check OUT
                                        </td>
                                        <td style="display:none;">
                                        </td>
                                    </tr>
                                </thead>
                                <t t-set="seq" t-value="1"/>

                                <tr t-foreach="sale_order_id.hotels_expense_ids" t-as="hotels">
                                    <td style="border: 1px solid; text-align:center; padding:5px;">
                                        <span t-esc="seq"/>
                                    </td>

                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="hotels.name">
                                            <span t-esc="hotels.name.name"/>
                                        </t>
                                    </td>
                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="hotels.date_from">
                                            <span t-esc="hotels.date_from"
                                                  t-options='{"widget": "date","format": "dd/MM/yyyy"}'/>
                                        </t>
                                    </td>
                                    <td style="border: 1px solid; padding:5px;">
                                        <t t-if="hotels.date_to">
                                            <span t-esc="hotels.date_to"
                                                  t-options='{"widget": "date","format": "dd/MM/yyyy"}'/>
                                        </t>
                                    </td>
                                    <td style="display:none;">
                                    </td>
                                    <t t-set="seq" t-value="seq +1"/>
                                </tr>
                            </table>
                        </t>

                        <!--                        <div>-->
                        <!--                            <h6-->
                        <!--                            style="-->
                        <!--                                  font-size: 1rem;-->
                        <!--                                  font-weight: bold;-->
                        <!--                                  margin-top: 65px;-->
                        <!--                                  "-->
                        <!--                            >Kind regards</h6>-->
                        <!--                            <p style="margin-bottom: 0rem;">Admin</p>-->
                        <!--                            <p style="margin-bottom: 0rem;">General Manger</p>-->
                        <!--                            <p style="margin-bottom: 0rem;">Email</p>-->
                        <!--                        </div>-->

                    </div>
                </t>
            </t>


        </template>
    </data>
</odoo>

